@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    @*<title></title>*@

    <link rel="shortcut icon" href="@Url.Content("~/Images/Zhoolego-Material-Games.ico")" type="image/x-icon">
    <!-- Bootstrap -->
    @RenderSection("pagetitle", required: false)



    <script src="@Url.Content("~/Assets/Client/js/JQueryV1.9.1.js")"></script>

    <link href="@Url.Content("~/Assets/Client/css/bootstrap.min.css")" rel="stylesheet">
    <!-- for fontawesome icon css file -->
    <link href="@Url.Content("~/Assets/Client/css/font-awesome.min.css")" rel="stylesheet">
    <!-- for content animate css file -->
    <link rel="/Assets/Client/stylesheet" href="@Url.Content("~/Assets/Client/css/animate.css")">
    <!-- google fonts  -->
    <link href='http://fonts.googleapis.com/css?family=Oswald' rel='stylesheet' type='text/css'>
    <!-- slick slider css file -->
    <link href="@Url.Content("~/Assets/Client/css/slick.css")" rel="stylesheet">
    <!-- <link href="css/theme-red.css" rel="stylesheet"> -->
    <link href="@Url.Content("~/Assets/Client/css/theme.css")" rel="stylesheet">
    <!-- main site css file -->
    <link href="@Url.Content("~/Assets/Client/style.css")" rel="stylesheet">

    @*facebook comment responsive*@
    <style>
        .fb-comments, .fb-comments iframe[style], .fb-like-box, .fb-like-box iframe[style], .fb-comments span, .fb-comments iframe span[style], .fb-like-box span, .fb-like-box iframe span[style] {
            width: 100% !important;
        }
    </style>



</head>
<body>

    <div id="preloader">
        <div id="status">&nbsp;</div>
    </div>

    <a class="scrollToTop" href="#"><i class="fa fa-angle-up"></i></a>

    <div class="container">
        <!-- start header area -->
        <header id="header">
            <div class="row">
                <div class="col-lg-12 col-md-12">
                    <!-- start header bottom -->
                    <div class="header_bottom">
                        <div class="header_bottom_left">
                            <a class="logo" href="/">
                                <strong></strong> <span style="font-family: Arial;">GÓC NHÌN CÔNG NGHỆ</span>
                            </a>
                        </div>

                        <div class="header_top_right">
                            <form class="search_form">
                                <input type="dest" class="form-control" placeholder="Tìm kiếm trên Góc Nhìn Công Nghệ . . ." id="txtSearch" name="txtSearch">
                                <span class="glyphicon glyphicon-search submit" aria-hidden="true">    </span>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </header><!-- End header area -->
        <!-- Static navbar -->
        <div id="navarea">
            <nav class="navbar navbar-default" role="navigation">
                <div class="container-fluid">
                    @{Html.RenderAction("MainMenu", "Home");}

                </div><!--/.container-fluid -->
            </nav>
        </div>
        <!-- start site main content -->
        <section id="mainContent">
            @RenderBody()
            <!-- start main content top -->
        </section>
    </div>

    <footer id="footer">
        <div class="footer_top">
            <div class="container">
                <div class="row">
                    <div class="col-lg-4 col-md-4 col-sm-4">
                        <div class="single_footer_top wow fadeInRight">
                            <p>Bài tập lớn môn Lập Trình Web 
                                <br /> Trần Văn Hải <br />MSV: V181512647 <br />Lớp: CNTT4 - K59
                                <br /> Giảng viên: Cô Nguyễn Kim Sao
                            </p>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-4">
                        <div class="single_footer_top wow fadeInRight">
                            <h2>Thành viên</h2>
                            <a href="/Home/Register" class="footer_link" target="_blank">Đăng ký</a> <br />
                            <a href="/Dang-Nhap/" class="footer_link" target="_blank">Đăng nhập</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="footer_bottom">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                        <div class="footer_bottom_left">
                            <p>Copyright © 2021 Trần Văn Hải-ĐH Giao Thông Vận Tải HN</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <script src="@Url.Content("~/Assets/Client/js/JQueryV1.9.1.js")"></script>
    <!-- jQuery google CDN Library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <!-- For content animatin  -->
    <script src="@Url.Content("~/Assets/Client/js/wow.min.js")"></script>
    <!-- bootstrap js file -->
    <script src="@Url.Content("~/Assets/Client/js/bootstrap.min.js")"></script>
    <!-- slick slider js file -->
    <script src="@Url.Content("~/Assets/Client/js/slick.min.js")"></script>

    <!-- custom js file include -->
    <script src="@Url.Content("~/Assets/Client/js/custom.js")"></script>

</body>
</html>